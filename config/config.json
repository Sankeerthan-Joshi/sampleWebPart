{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "count-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/count/CountWebPart.js",
          "manifest": "./src/webparts/count/CountWebPart.manifest.json"
        }
      ]
    },
    "countdown-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/countdown/CountdownWebPart.js",
          "manifest": "./src/webparts/countdown/CountdownWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "CountWebPartStrings": "lib/webparts/count/loc/{locale}.js",
    "CountdownWebPartStrings": "lib/webparts/countdown/loc/{locale}.js"
  }
}
